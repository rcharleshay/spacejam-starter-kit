  "scripts": {
    "clean:client": "rimraf dist && mkdir -p dist",
    "clean:server": "rimraf server-dist && mkdir -p server-dist",
    "webpack:init": "npm-run-all clean:*",
    "webpack:client": "NODE_ENV=production webpack --config webpack.client.config.js --progress --colors",
    "webpack:server": "NODE_ENV=production webpack --config webpack.server.config.js --progress --colors",
    "build": "npm-run-all webpack:init webpack:client webpack:server",
    "start": "NODE_ENV=production node server-dist/server.js --colors",
    "contrast": "node-contrast server-dist/server.js -c contrast/contrast.yaml --server.environment=$APP_ENV -- --colors",
    "webpack:client:dev": "NODE_ENV=development webpack --config webpack.client.config.js --colors --debug --output-pathinfo --devtool inline-source-map",
    "webpack:server:dev": "NODE_ENV=development webpack --config webpack.server.config.js --colors --debug --output-pathinfo --devtool inline-source-map",
    "webpack:dev": "npm-run-all webpack:init webpack:watch",
    "webpack:watch": "npm-run-all --parallel 'webpack:*:dev -- --watch'",
    "start:dev": "APP_ENV=development NODE_ENV=development nodemon --watch dist --watch server-dist --ext js server-dist/server.js",
    "development": "npm-run-all --parallel webpack:dev start:dev",
    "dev": "npm run development",
    "test": "npm-run-all lint unit && npm audit",
    "test:sonarqube": "npm-run-all test sonarqube",
    "lint": "npx eslint 'src' --config .eslintrc --ext .js,.jsx",
    "lint:fix": "npm run lint --fix",
    "unit": "APP_ENV=development BABEL_ENV=test jest -i",
    "unit:clear": "npm run unit -- -u",
    "unit:watch": "npm run unit --watchAll",
    "preanalyze": "NODE_ENV=production webpack --config webpack.client.config.js --profile --json > .analyze.json",
    "analyze": "webpack-bundle-analyzer -s parsed .analyze.json",
    "sonarqube": "sonarqube-scanner-node -Dsonar.login=${SONARQUBE_TOKEN} -Dsonar.projectVersion=${VERSION:-local}",
    "commit": "git-cz"
  }
